<?xml version="1.0" encoding="utf-8"?>
<Configuration name="fitness-network">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %p %marker %m %X %ex %n" />
        </Console>
    </Appenders>
    <Loggers>
        <!-- Default logger -->
        <Root level="info">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>
